# GPU settings
QWEN_GPU=0
TRELLIS_GPU=0
QWEN_DTYPE=bf16

# Output settings
SAVE_GENERATED_FILES=false
SEND_GENERATED_FILES=false
OUTPUT_DIR=generated_outputs

# Trellis model settings
TRELLIS_MODEL_ID=jetx/trellis-image-large
TRELLIS_SPARSE_STRUCTURE_STEPS=8
TRELLIS_SPARSE_STRUCTURE_CFG_STRENGTH=5.75
TRELLIS_SLAT_STEPS=20
TRELLIS_SLAT_CFG_STRENGTH=2.4
COMPRESSION=true

# Background Removal Settings - Tuned for Multi-Object & Transparent Objects
# Lower threshold (0.3-0.6) detects transparent objects like glass better
# Higher threshold (0.7-0.9) focuses on solid, opaque objects only
MASK_THRESHOLD=0.5
MASK_THRESHOLD_MIN=0.3
MASK_THRESHOLD_MAX=0.95
USE_ADAPTIVE_THRESHOLD=true

# Object Detection Settings - Relaxed for multi-object scenes
MIN_OBJECT_COVERAGE=0.02
MAX_OBJECT_COVERAGE=0.98

# Padding Settings
PADDING_PERCENTAGE=0.2
LIMIT_PADDING=true
USE_SMART_PADDING=true
ADAPTIVE_PADDING_FACTOR=1.5

# Transparency Enhancement for Glass Objects
# Detects glass/transparent regions and increases their transparency
ENABLE_TRANSPARENCY_BOOST=true
TRANSPARENCY_DETECTION_THRESHOLD=0.15
TRANSPARENCY_BOOST_FACTOR=0.6
EDGE_PRESERVE_WIDTH=10

# Qwen model settings
QWEN_EDIT_BASE_MODEL_PATH=Qwen-Image-Edit-2509/Qwen-Image-Edit-2509-Lightning-4steps-V1.0-bf16.safetensors
QWEN_EDIT_MODEL_PATH=Qwen/Qwen-Image-Edit-2509
